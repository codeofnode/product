{
  "name": "tcgen",
  "version": "0.0.0",
  "description": "A unit test case generator. It will intercept the methods of class and save the input and out to file.",
  "git": {
    "owner": "codeofnode",
    "hostedOn": "github.com",
    "url": "git@{{git.hostedOn}}/{{git.owner}}/{{name}}.git"
  },
  "keywords": [
    "unit",
    "testcase",
    "interceptor",
    "class",
    "file",
    "method",
    "function"
  ],
  "license": "MIT",
  "author": {
    "name": "Ramesh Kumar",
    "email": "codeofnode@gmail.com"
  },
  "org": {
    "domain": "github.com",
    "scheme": "https",
    "url": "{{org.scheme}}://{{org.domain}}"
  },
  "srcDir": "src",
  "module": {
    "dirName": "modules",
    "path": "{{srcDir}}/{{module.dirName}}",
    "prefix": "product-",
    "dependencies": {
      "petu": 1
    }
  },
  "config": {
    "modulePrefix": "{{module.prefix}}"
  },
  "homepage": "{{org.url}}/codeofnode/product/tree/{{name}}"
}
