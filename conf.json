{
  "name": "allrounder",
  "version": "1.0.0",
  "description": "It validate everything. From rest call to dbvalidation to system commands to unit tests. Just almost everything.",
  "git": {
    "owner": "codeofnode",
    "hostedOn": "github.com",
    "url": "git@{{git.hostedOn}}/{{git.owner}}/{{name}}.git"
  },
  "keywords": [
    "rest",
    "unit",
    "test",
    "json",
    "db",
    "system",
    "upload"
  ],
  "license": "MIT",
  "author": {
    "name": "Ramesh Kumar",
    "email": "codeofnode@gmail.com"
  },
  "org": {
    "domain": "github.com",
    "scheme": "https",
    "url": "{{org.scheme}}://{{org.domain}}"
  },
  "srcDir": "src",
  "module": {
    "dirName": "modules",
    "path": "{{srcDir}}/{{module.dirName}}",
    "prefix": "product-",
    "dependencies": {
      "petu": 2,
      "json2server": 1
    }
  },
  "config": {
    "modulePrefix": "{{module.prefix}}"
  },
  "homepage": "{{org.url}}/codeofnode/product/tree/{{name}}"
}
